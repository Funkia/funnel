{
  "name": "@funkia/turbine",
  "version": "0.3.0",
  "description": "A functional frontend framework in Typescript",
  "main": "dist/cmjs/index.js",
  "module": "dist/es6/index.js",
  "typings": "dist/defs/index.d.ts",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --coverage",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "codecov": "codecov -f coverage/**/json",
    "build-es6": "tsc -P tsconfig-build.json --module ES6 --outDir dist/es6 --declaration true --declarationDir ./dist/defs",
    "build-cmjs": "tsc -P tsconfig-build.json",
    "build": "npm run build-es6; npm run build-cmjs",
    "clean": "rm -rf coverage dist",
    "prepublishOnly": "npm run clean; npm run build",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funkia/turbine.git"
  },
  "keywords": [
    "pure",
    "functional",
    "framework",
    "frp",
    "functional reactive programming",
    "typescript"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@funkia/hareactive": "0.3.x"
  },
  "dependencies": {
    "@funkia/jabz": "0.0.24"
  },
  "bugs": {
    "url": "https://github.com/funkia/turbine/issues"
  },
  "homepage": "https://github.com/funkia/turbine#readme",
  "devDependencies": {
    "@funkia/hareactive": "^0.3.3",
    "@types/chai": "^4.1.7",
    "@types/chai-dom": "0.0.7",
    "@types/jest": "^24.0.15",
    "chai": "^4.2.0",
    "chai-dom": "^1.8.1",
    "codecov": "^3.3.0",
    "fake-raf": "^1.0.1",
    "jest": "^24.8.0",
    "np": "^5.0.1",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "contributors": [
    {
      "name": "Simon Friis Vindum"
    },
    {
      "name": "Emil Gj√∏rup",
      "email": "limemloh@gmail.com"
    }
  ],
  "prettier": {
    "arrowParens": "always"
  }
}
